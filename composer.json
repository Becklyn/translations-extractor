{
    "name": "becklyn/translations-extractor",
    "description": "Extracts translations from the given directory.",
    "license": "BSD-3-Clause",
    "type": "lib",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "homepage": "https://github.com/Becklyn/translations-extractor",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "php-translation/extractor": "^2.1.0",
        "symfony/console": "^v5.4.3 || ^v6.0.3",
        "symfony/finder": "^v5.4.3 || ^v6.0.3",
        "symfony/form": "^v5.4.3 || ^v6.0.3",
        "symfony/twig-bridge": "^v5.4.3 || ^v6.0.3",
        "symfony/validator": "^v5.4.3 || ^v6.0.3",
        "twig/twig": "^3.3.8"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^v5.4.3 || ^v6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\TranslationsExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\TranslationsExtractor\\": "tests/"
        }
    },
    "bin": [
        "bin/extract-translations"
    ],
    "config": {
        "sort-packages": true
    }
}
